{
  "name": "@jwynia/corticai",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "test": "vitest run --config vitest.config.unit.ts",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:watch": "vitest --config vitest.config.unit.ts",
    "test:ui": "vitest --ui --config vitest.config.unit.ts",
    "test:coverage": "vitest run --config vitest.config.unit.ts --coverage",
    "verify:integration": "echo '⚠️  Integration tests indicate architectural issues. Consider refactoring for better testability.' && vitest run --config vitest.config.integration.ts",
    "lint": "tsc --noEmit",
    "lint:check": "tsc --noEmit --skipLibCheck",
    "type-check": "tsc --noEmit",
    "dev": "mastra dev",
    "build": "tsc --project tsconfig.build.json",
    "start": "mastra start",
    "mcp:server": "tsx src/context/mcp/stdio.ts",
    "mcp:build": "tsc src/context/mcp/stdio.ts --outDir dist --module esnext --target es2022",
    "docs": "typedoc",
    "docs:serve": "cd docs && python3 -m http.server 8080",
    "docs:watch": "typedoc --watch",
    "docs:json": "typedoc --json docs/documentation.json",
    "docs:clean": "rm -rf docs",
    "docs:build": "npm run docs:clean && npm run docs",
    "docs:deploy": "npm run docs:build && gh-pages -d docs",
    "benchmark": "npx tsx ./benchmarks/cli.ts",
    "benchmark:query": "npm run benchmark -- --suites query",
    "benchmark:storage": "npm run benchmark -- --suites storage",
    "benchmark:comparison": "npm run benchmark -- --suites comparison",
    "benchmark:fast": "npm run benchmark -- --iterations 5 --max-time 10",
    "benchmark:memory": "npm run benchmark -- --adapters Memory",
    "benchmark:json": "npm run benchmark -- --adapters JSON",
    "benchmark:duckdb": "npm run benchmark -- --adapters DuckDB",
    "benchmark:small": "npm run benchmark -- --data-size 1K",
    "benchmark:regression": "npm run benchmark -- --format json && npx tsx ./benchmarks/regression-check.ts"
  },
  "keywords": [
    "typescript",
    "llm",
    "agent",
    "duckdb",
    "storage",
    "mastra",
    "query",
    "ai"
  ],
  "author": "J Wynia",
  "license": "ISC",
  "description": "TypeScript LLM Agent context management framework with DuckDB and JSON storage adapters.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwynia/corticai.git"
  },
  "bugs": {
    "url": "https://github.com/jwynia/corticai/issues"
  },
  "homepage": "https://github.com/jwynia/corticai#readme",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./context": "./dist/context/index.js",
    "./context/tools": "./dist/context/tools/index.js",
    "./context/agents": "./dist/context/agents/index.js",
    "./context/mcp": "./dist/context/mcp/index.js",
    "./storage": "./dist/storage/index.js",
    "./query": "./dist/query/index.js",
    "./mastra": "./dist/mastra/index.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.30",
    "@azure/cosmos": "^4.5.1",
    "@azure/identity": "^4.12.0",
    "@duckdb/node-api": "^1.3.4-alpha.27",
    "@mastra/core": "^0.16.3",
    "@mastra/libsql": "^0.14.1",
    "@mastra/loggers": "^0.10.11",
    "@mastra/memory": "^0.15.1",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@types/js-yaml": "^4.0.9",
    "chokidar": "^4.0.3",
    "duckdb": "^1.3.4",
    "file-type": "^21.0.0",
    "js-yaml": "^4.1.0",
    "kuzu": "^0.11.3",
    "mime-types": "^3.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^24.3.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "benchmark": "^2.1.4",
    "gh-pages": "^6.3.0",
    "happy-dom": "^18.0.1",
    "mastra": "^0.12.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
