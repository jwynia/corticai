{
  "name": "corticai",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "dev": "mastra dev",
    "build": "mastra build",
    "start": "mastra start",
    "docs": "typedoc",
    "docs:serve": "cd docs && python3 -m http.server 8080",
    "docs:watch": "typedoc --watch",
    "docs:json": "typedoc --json docs/documentation.json",
    "docs:clean": "rm -rf docs",
    "docs:build": "npm run docs:clean && npm run docs",
    "docs:deploy": "npm run docs:build && gh-pages -d docs",
    "benchmark": "npx tsx ./benchmarks/cli.ts",
    "benchmark:query": "npm run benchmark -- --suites query",
    "benchmark:storage": "npm run benchmark -- --suites storage",
    "benchmark:comparison": "npm run benchmark -- --suites comparison",
    "benchmark:fast": "npm run benchmark -- --iterations 5 --max-time 10",
    "benchmark:memory": "npm run benchmark -- --adapters Memory",
    "benchmark:json": "npm run benchmark -- --adapters JSON",
    "benchmark:duckdb": "npm run benchmark -- --adapters DuckDB",
    "benchmark:small": "npm run benchmark -- --data-size 1K",
    "benchmark:regression": "npm run benchmark -- --format json && npx tsx ./benchmarks/regression-check.ts"
  },
  "keywords": [
    "typescript",
    "llm",
    "agent",
    "duckdb",
    "storage",
    "mastra",
    "query",
    "ai"
  ],
  "author": "CorticAI Team",
  "license": "ISC",
  "description": "TypeScript LLM Agent Framework with DuckDB storage, advanced query interfaces, and Mastra integration",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@duckdb/node-api": "^1.3.4-alpha.27",
    "@mastra/core": "^0.10.8",
    "@mastra/libsql": "^0.11.0",
    "@mastra/loggers": "^0.10.2",
    "@mastra/memory": "^0.11.0",
    "@openrouter/ai-sdk-provider": "^0.7.2",
    "duckdb": "^1.3.4",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^24.0.7",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "benchmark": "^2.1.4",
    "gh-pages": "^6.3.0",
    "happy-dom": "^18.0.1",
    "mastra": "^0.10.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.12",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
