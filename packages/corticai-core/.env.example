# CorticAI Core Configuration

# Environment
NODE_ENV=development

# Database
KUZU_DB_PATH=./data/kuzu.db

# Memory Tiers
HOT_CACHE_TTL=300
WARM_STORAGE_TTL=3600
COLD_ARCHIVE_CONSOLIDATION_INTERVAL=7200

# Performance
MAX_PARALLEL_PARSERS=4
QUERY_TIMEOUT_MS=5000

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/corticai.log

# Development
DEBUG=false
ENABLE_PROFILING=false